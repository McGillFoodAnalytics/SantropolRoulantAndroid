<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".TimePreference">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/lg_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/rg_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tg_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tg_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.14" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tg_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tg_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.36" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/tg_frag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.47" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/bg_frag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="1.0" />


    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.MapFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:context=".ContactUs"
        app:layout_constraintBottom_toBottomOf="@+id/bg_frag"
        app:layout_constraintLeft_toLeftOf="@+id/lg_main"
        app:layout_constraintRight_toRightOf="@+id/rg_main"
        app:layout_constraintTop_toTopOf="@+id/tg_frag" />

    <TextView
        android:id="@+id/tvAddress"
        style="@style/text_view_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/light_purple"
        android:drawableLeft="@drawable/location"
        android:drawablePadding="@dimen/spacing_large"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/spacing_large"
        android:text="@string/_111_roy_east_montreal_qc_h2w_1m1"
        app:layout_constraintBottom_toBottomOf="@+id/tg_text2"
        app:layout_constraintLeft_toLeftOf="@+id/lg_main"
        app:layout_constraintRight_toRightOf="@+id/rg_main"
        app:layout_constraintTop_toTopOf="@+id/tg_text1" />
    <TextView
        android:id="@+id/tvEmail"
        style="@style/text_view_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/light_purple"
        android:drawableLeft="@drawable/email"
        android:drawablePadding="@dimen/spacing_large"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/spacing_large"
        android:text="@string/info_santropolroulant_org"
        app:layout_constraintBottom_toBottomOf="@+id/tg_frag"
        app:layout_constraintLeft_toLeftOf="@+id/lg_main"
        app:layout_constraintRight_toRightOf="@+id/rg_main"
        app:layout_constraintTop_toTopOf="@+id/tg_text3" />
    <TextView
        android:id="@+id/tvPhoneNumber"
        style="@style/text_view_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/light_purple"
        android:drawableLeft="@drawable/phone"
        android:drawablePadding="@dimen/spacing_large"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/spacing_large"
        android:text="@string/_514_284_9335"
        app:layout_constraintBottom_toBottomOf="@+id/tg_text3"
        app:layout_constraintLeft_toLeftOf="@+id/lg_main"
        app:layout_constraintRight_toRightOf="@+id/rg_main"
        app:layout_constraintTop_toTopOf="@+id/tg_text2" />
    <TextView
        android:id="@+id/tvTitle2"
        style="@style/header_bold"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="@string/let_s_get_in_touch"
        android:textSize="@dimen/_25sdp"
        app:layout_constraintBottom_toBottomOf="@+id/tg_text1"
        app:layout_constraintLeft_toLeftOf="@+id/lg_main"
        app:layout_constraintRight_toRightOf="@+id/rg_main"
        app:layout_constraintTop_toTopOf="@+id/tg_header" />


</androidx.constraintlayout.widget.ConstraintLayout>